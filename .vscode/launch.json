{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch ASP.NET Core Web API",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/OrderApp.Web/bin/Debug/net9.0/OrderApp.Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "uri": "https://localhost:5001/swagger"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}
